let cargoHold=['oxygen tanks','space suits','parrot','instruction manual','meal packs','slinky','security blanket'];
console.log(cargoHold);
cargoHold[cargoHold.indexOf('slinky')]='space tether';
console.log(cargoHold);
cargoHold.pop();
console.log(cargoHold);
cargoHold.shift();
console.log(cargoHold);
cargoHold.push(1138);
console.log(cargoHold);
cargoHold.unshift('20 meters');
console.log(cargoHold);
